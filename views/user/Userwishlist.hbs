      <div class="cart-wrap">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <div class="main-heading mb-10">My wishlist</div>
              <div class="table-wishlist">
                <table cellpadding="0" cellspacing="0" border="0" width="100%">
                  <thead>
                    <tr>
                      <th width="30%">Product Name</th>
                      <th width="15%">Unit Price</th>
                      <th width="15%">Product Availabilty</th>
                      <th width="15%" style="padding-left: 15%;">Actions</th>
                      <th width="10%"></th>
                    </tr>
                  </thead>
                  {{#each wishlistdata}}
                  <tbody>
                    <tr>
                      <td width="30%">
                        <div class="display-flex align-center">
                          <div class="img-product">
                            <img src="../../images/{{this.product.product.file.image1.[0].filename}}" alt="" class="mCS_img_loaded kk">
                          </div>
                          <div class="name-product">
                            {{this.product.product.productname}}
                          </div>
                        </div>
                      </td>
                      <td width="15%" class="price">{{#eq this.product.product.price '0' }}
                                    <div class="d-flex justify-content-center">
                                        <h6>₹{{this.product.product.price}}</h6>
                                        {{!-- <h6 class="text-muted ml-2"><del>₹</del></h6> --}}
                                    </div>
                                  {{else}}
                                    <div class="d-flex justify-content-center">
                                        {{#when this.product.catOffer 'gt' this.product.proOffer}}
                                        <h6>{{this.product.catOffer}}%OFF</h6><br>
                                        <h6 class="text-muted ml-2"><del>{{this.product.price}}</del></h6>
                                      <h6>₹{{this.product.product.discprice}}</h6> 
                                        {{else}}
                                           <h6>{{this.proOffer}}%OFF</h6> 
                                        <h6 class="text-muted ml-2"><del>{{this.product.price}}</del></h6> 
                                      <h6>₹{{this.product.product.discprice}}</h6> 
                                    </div>
                                       {{/when}}
                                   
                                    {{/eq}}</td>

                      <td width="15%" style="padding-left: 5%;">

                        <span class="">

                        {{#if this.product.stockstatus}}
                          In Stock
                          {{else}}
                           No stock
                        {{/if}}


                        </span>
                      </td>
                      <td width="15%"> 
                        
                          {{#if this.product.stockstatus}}
                           <a style="text-decoration:none ;" class="round-black-btn small-btn " href="/Addtocartwish/{{this.product._id}}">
                          Add to cart</a>
                          {{else}}
                           <p  style="font-size: large;  font-weight: 500 ;" >  WE WILL ALERT YOU</p>
                        {{/if}}
                        </td>
                      <td width="10%" class="text-center">

                            
                        <button
                          onclick="wishdel('{{this._id}}','{{this.product._id}}')"
                          class="round-black-btn ">Remove</button>
                        
                          
                       
                          </td>
                    </tr>


                  </tbody>
                  {{/each}}
                </table>
              </div>
            </div>
          </div>
        </div>
